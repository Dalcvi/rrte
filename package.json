{
  "name": "@rrte/root",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "watch": "turbo run watch",
    "lint": "turbo run lint",
